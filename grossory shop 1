import java.io.*;
import java.util.*;
import java.text.*;
import java.math.*;
import java.util.regex.*;

public class Solution {

    public static void main(String[] args) {
        Scanner sc=new Scanner(System.in);
        
        System.out.println("Items Details");
        String a=sc.next();
        System.out.println("A4sheet:"+a);
        String b=sc.next();
        System.out.println("Pen:"+b);
        String c=sc.next();
        System.out.println("Pencil:"+c);
        String d=sc.next();
        System.out.println("Eraser:"+d);
    }
}
